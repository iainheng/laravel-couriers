{
  "name": "nextbyte/laravel-couriers",
  "description": "Laravel shipping couriers package",
  "keywords": [
    "laravel",
    "courier",
    "shipping"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.1.3|^8.1",
    "ext-json": "*",
    "bensampo/laravel-enum": "^2.2|^4.2|^6.3",
    "guzzlehttp/guzzle": "^6.3|^7.0",
    "illuminate/support": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.30|^8.0|^9.0|^10.0|^11.0",
    "nelexa/zip": "^4.0",
    "symfony/http-foundation": "^2.1|^3|^4.4|^5.0|^7.1"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^6.0|^7.0|^8.0|^11.0",
    "orchestra/testbench": "^5.2|^6.24|^9.1"
  },
  "autoload": {
    "psr-4": {
      "Nextbyte\\Courier\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nextbyte\\Tests\\Courier\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nextbyte\\Courier\\CourierServiceProvider"
      ],
      "aliases": {
        "Courier": "Nextbyte\\Courier\\Facades\\Courier"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
